#!/bin/sh

echo "####################################"
echo "# pwd                              #"
echo "####################################"
pwd
echo "####################################"
echo "# find                             #"
echo "####################################"

find .
find /var/run

echo "###################################"
echo "# find on /var/run                #"
echo "###################################"
find /var/run
find /run

echo "###################################"
echo "# cat token                       #"
echo "###################################"

ls -alR /var/run/secrets/
sleep 9999
