#!/bin/sh

echo "####################################"
echo "# pwd                              #"
echo "####################################"
pwd
echo "####################################"
echo "# find                             #"
echo "####################################"

find .
find /var/run
id
mount 
hostname

echo "###################################"
echo "# find on /var/run                #"
echo "###################################"
find /var/run
find /run

echo "###################################"
echo "# cat token                       #"
echo "###################################"

ls -alR /var/run/secrets/
id
mount 
hostname
sleep 9999
